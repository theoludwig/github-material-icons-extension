{
  "name": "Material Icons for GitHub (fork for custom use case)",
  "version": "1.8.13",
  "description": "Replace the file/folder icons on the GitHub file browser with icons representing the file's type and which tool it is used by.",
  "homepage_url": "https://github.com/theoludwig/github-material-icons-extension/tree/custom",
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://github.com/*",
        "*://bitbucket.org/*",
        "*://dev.azure.com/*",
        "*://*.visualstudio.com/*",
        "*://gitea.com/*",
        "*://git.theoludwig.fr/*",
        "*://gitlab.com/*",
        "*://gitee.com/*",
        "*://sourceforge.net/*"
      ],
      "js": ["./main.js"],
      "css": ["./injected-styles.css"],
      "run_at": "document_start"
    }
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "permissions": ["storage", "activeTab"]
}
